{
  "name": "rubics-app-express-template",
  "license": "UNLICENCED",
  "private": true,
  "type": "module",
  "engines": {
    "node": "14.20.0"
  },
  "scripts": {
    "start": "node dist/lib/server.js",
    "dev": "nodemon",
    "web": "vite build --watch",
    "build": "vite build && tsc --project tsconfig.prod.json",
    "storybook": "npx storybook dev -p 9004",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:ui": "vitest --ui --api 9527",
    "test:run": "vitest run",
    "seed": "node --loader ts-node/esm ./seed.ts",
    "webstorm-integration": "vitest --watch --reporter=dot --reporter=json --outputFile=.vitest-result.json"
  },
  "dependencies": {
    "@ludens-reklame/rubics-app-express": "^0.1.1",
    "@ludens-reklame/rubics-app-sdk": "^0.1.2",
    "@ludens-reklame/rubics-sdk": "^1.25.0",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "express-static-gzip": "^2.1.7",
    "mongoose": "^6.6.1",
    "node-fetch": "^3.2.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@ludens-reklame/rubics-app-bridge": "^1.0.1",
    "@ludens-reklame/rubics-theme": "^1.0.9",
    "@ludens-reklame/vitest-mockify": "^0.2.7",
    "@luncheon/esbuild-plugin-gzip": "^0.1.0",
    "@types/babel__core": "^7.1.19",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/node": "14.18.28",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/ui": "^0.23.2",
    "esbuild": "^0.15.7",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mongodb-memory-server": "^8.9.1",
    "msw": "^0.47.2",
    "prettier": "^2.7.1",
    "react-router": "^6.2.2",
    "react-router-dom": "^6.2.2",
    "react-useportal": "^1.0.16",
    "rollup-plugin-livereload": "^2.0.5",
    "styled-components": "5.3.5",
    "ts-node": "^10.9.1",
    "tsx": "^3.9.0",
    "typescript": "4.6.2",
    "vite": "^3.1.1",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^0.23.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
